{
  "manifest_version": 2,
  "name": "ytAutoDark",
  "version": "0.0.3",
  "author": "Victor Voisin",
  "description": "Automatically switch Youtube to its built-in dark theme.",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["ytAutoDark.js"]
    }
  ],
  "applications": {
    "gecko": {
      "id": "vvision-ytAutoDark@unicoda.fr"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "60.0"
    }
  }
}
