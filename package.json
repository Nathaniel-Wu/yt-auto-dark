{
  "name": "ytAutoDark",
  "version": "2.0.0",
  "description": "Automatically switch Youtube to its built-in dark theme.",
  "author": "Victor Voisin",
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/vvision/yt-auto-dark"
  },
  "bugs": {
    "url": "https://github.com/vvision/yt-auto-dark/issues"
  },
  "scripts": {
    "build": "npm run noDevDebug && npm run lint && web-ext build -s src -o",
    "lint": "web-ext lint -s src",
    "start:firefox": "web-ext run --source-dir ./src/ --start-url www.youtube.com",
    "noDevDebug": "node utils/isDebugDisable.js"
  },
  "devDependencies": {
    "@unicoda/eslint-config": "^0.9.1",
    "@unicoda/prettier-config": "^0.2.0",
    "eslint": "^6.8.0",
    "prettier": "^2.0.5",
    "web-ext": "^4.2.0"
  }
}
